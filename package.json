{
    "name": "egorptashnik",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "npm-run-all --parallel start-ui start-srv",
        "start-ui": "node node_modules/@sap/approuter/approuter.js",
        "start-srv": "cds watch",
        "deploy": "cds deploy"
    },
    "dependencies": {
        "@sap/approuter": "^14.1.1",
        "@sap/cds-dk": "^6.8.3",
        "nodemon": "^2.0.22",
        "npm-run-all": "^4.1.5"
    },
    "cds": {
        "requires": {
            "db": {
                "kind": "sqlite",
                "model": [ "db", "srv/services" ]
            },
            "odata": {
                "version": "v4"
            }
        }
    },
    "files": [ "db", "srv" ]
}
