<mvc:View controllerName="yp.landing.controller.Home"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}">
        <headerContent>
            <ToolbarSeparator id="pageToolarSeparator"/>
            <Switch id="pageSwitchTheme" customTextOff=" " customTextOn=" " change="toggleTheme"/>
        </headerContent>
        <content>
            <VBox id="homeVBoxMain" class="sapUiResponsiveMargin">
                <core:Fragment fragmentName="yp.landing.view.HomeFragments.Header"/>
                <core:Fragment fragmentName="yp.landing.view.HomeFragments.HeaderMobile"/>
                <core:Fragment fragmentName="yp.landing.view.HomeFragments.Skills"/>
            </VBox>
        </content>
    </Page>
</mvc:View>
