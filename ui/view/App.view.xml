<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.tnt"
    xmlns:m="sap.m"
    height="100%"
    displayBlock="true"
    controllerName="yp.controller.App">
    <ToolPage>
        <header>
            <ToolHeader>
                <m:ToolbarSpacer/>
                <m:OverflowToolbarButton icon="sap-icon://settings"/>
            </ToolHeader>
        </header>
        <subHeader>
            <ToolHeader>
                <m:IconTabHeader>
                    <m:items>
                        <m:IconTabFilter text="Todo"/>
                    </m:items>
                </m:IconTabHeader>
            </ToolHeader>
        </subHeader>
        <mainContents>
            <m:App id="app"/>
        </mainContents>
    </ToolPage>

</mvc:View>
