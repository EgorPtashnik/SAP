<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.f"
    displayBlock="true"
    controllerName="yp.controller.home.Home">
    
    <DynamicPage>
        <title>
            <core:Fragment type="XML" fragmentName="yp.view.home.Title"/>
        </title>

        <header>
            <core:Fragment type="XML" fragmentName="yp.view.home.Header"/>
        </header>

        <content>
            <core:Fragment type="XML" fragmentName="yp.view.home.Content"/>
        </content>

        <footer>
            <core:Fragment type="XML" fragmentName="yp.view.home.Footer"/>
        </footer>
    </DynamicPage>

</mvc:View>
