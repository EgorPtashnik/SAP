<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Dialog title="Delete Confirmation" type="Message" state="Warning">
        <content>
            <Text text="Please confirm the deletion"/>
        </content>
        <buttons>
            <Button text="Submit" press="onConfirmDeleteList"/>
            <Button text="Cancel" press="onDialogClose"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>